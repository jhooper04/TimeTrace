{
  "ConnectionStrings": {
    "DefaultConnection": "Server={DB_SERVER};Database={DB_SCHEMA};Uid={DB_USER};Pwd={DB_PASS};"
  },
  "JwtTokenSettings": {
    "ValidIssuer": "ExampleIssuer",
    "ValidAudience": "ExampleAudience",
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}